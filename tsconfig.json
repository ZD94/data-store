{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist/",
    "noEmitHelpers": true,
    "importHelpers": true,
    "sourceMap": true,
    "declaration": false,
    "alwaysStrict": true,
    "experimentalDecorators": true,
    "noImplicitUseStrict": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "lib": [
      "es2015",
      "dom"
    ]
  },
  "compilerOptionsNotUsing": {
    "emitDecoratorMetadata": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
  },
  "exclude": [
    "node_modules",
    "public",
    "dist",
    "ionic",
    "tmp",
    "*.bak",
    "*.tar.gz",
    "http/test",
    "**/*.test.ts"
  ],
  "excludeNotUsing": []
}