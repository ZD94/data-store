{
  "appName": "${JL_APP_NAME}",
  "rootDir": "${JL_APP_DATA_DIR}",
  "postgres": {
    "url": "${JL_APP_POSTGRES}",
    "debug": true
  },
  "cluster": true,
  "redis": {
    "url": "${JL_APP_REDIS}"
  },
  "hosts": {
    "dtask": {
      "addr": "l.jingli365.com",
      "port": 14108
    }
  },
  "api": {
    "debug": true,
    "remotes": [
      {
        "connect": {
          "host": "${{hosts.dtask.addr}}",
          "port": "${{hosts.dtask.port}}"
        },
        "modules": [
          "dtask_mgr"
        ]
      }
    ]
  },
  "logger": {
    "path": "${{rootDir}}/log",
    "prefix": "${{appName}}_",
    "httptype": "short",
    "mods": {
      "sequelize": {
        "prefix": "${{appName}}_sql_",
        "console": false
      },
      "sqlinit": {
        "prefix": "${{appName}}_sql_",
        "console": true
      },
      "http": {
        "mods": {
          "access": {
            "console": false
          }
        }
      }
    }
  },
  "listen": "4106",
  "socket_file": "${{rootDir}}/dataStoreWeb.sock",
  "placeAPI": "https://j.jingli365.com/proj/svr-place"
}